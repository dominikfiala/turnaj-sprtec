{
  "name": "turnaj-sprtec",
  "version": "1.1.0",
  "description": "Aplikace pro řízení turnaje ve šprtci",
  "main": "src/main.js",
  "build": {
    "appId": "cz.dominikfiala.${name}",
    "icon": "src/icon.ico",
    "directories": {
      "output": "./dist"
    },
    "win": {
      "target": "portable"
    }
  },
  "scripts": {
    "gulp": "./node_modules/gulp/bin/gulp.js",
    "build-test": "npm run prebuild && npx electron-builder --win --ia32 --dir",
    "prebuild": "npm run build-clean",
    "build": "npx electron-builder --win --ia32",
    "build-clean": "rm -rf build && rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dominikfiala/turnaj-sprtec.git"
  },
  "keywords": [
    "turnaj",
    "šprtec"
  ],
  "author": "Dominik Fiala, domo.fiala@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dominikfiala/turnaj-sprtec/issues"
  },
  "homepage": "https://github.com/dominikfiala/turnaj-sprtec#readme",
  "dependencies": {
    "bootstrap": "^4.0.0-beta.2",
    "edmonds-blossom": "^1.0.0",
    "exceljs": "^0.6.2",
    "faker": "^4.1.0",
    "file-saver": "^1.3.3",
    "jquery": "^3.2.1",
    "popper.js": "^1.13.0",
    "vue": "^2.5.9"
  },
  "devDependencies": {
    "browser-sync": "^2.18.13",
    "electron": "1.7.9",
    "electron-builder": "^19.48.2",
    "electron-connect": "^0.6.2",
    "electron-devtools-installer": "^2.2.1",
    "gulp": "github:gulpjs/gulp#4.0"
  }
}
