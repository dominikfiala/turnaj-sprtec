  "description": "Aplikace pro řízení turnaje ve šprtci",
  "main": "src/main.js",
  "build": {
    "appId": "cz.dominikfiala.${name}",
    "icon": "src/icon.ico",
    "directories": {
      "output": "./dist"
    },
    "win": {
