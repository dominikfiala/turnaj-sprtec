  "dependencies": {
    "bootstrap": "^4.0.0",
    "edmonds-blossom": "^1.0.0",
    "electron-devtools-installer": "^2.2.3",
    "exceljs": "^0.8.2",
    "faker": "^4.1.0",
    "file-saver": "^1.3.3",
    "jquery": "^3.3.1",
    "popper.js": "^1.13.0",
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "browser-sync": "^2.23.6",
